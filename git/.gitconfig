[user]
	email = raplonu.jb@gmail.com
	name  = raplonu
[core]
	excludesfile = /home/jbernard/.gitignore_global
	editor       = code --wait
[alias]
	st  = status
	co  = checkout
	ci  = commit
	br  = branch
	lg = log --graph --abbrev-commit --decorate --pretty=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)'
	llg = log --graph --abbrev-commit --decorate --pretty=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)'
