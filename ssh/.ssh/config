ForwardAgent      yes
ForwardX11        yes
ForwardX11Trusted yes
ForwardX11Timeout 8h
HashKnownHosts    no
Compression       yes
Ciphers           aes128-ctr
ServerAliveInterval 30

# Host samsara
#    ProxyCommand ssh -CY -q -W %h:%p styx.obspm.fr

Host samsara
    ProxyCommand ssh -q styx.obspm.fr nc -q0 -w 90 samsara 22

Host samsara.tun
    ProxyCommand ssh -q styx.obspm.fr nc -q0 -w 90 samsara 22
    LocalForward 8080 samsara.obspm.fr:8080